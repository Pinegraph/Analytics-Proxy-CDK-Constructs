{
  "name": "@pinegraph/analytics-proxy-cdk",
  "scripts": {
    "build": "tsc && cp src/constructs/google/gtag.js lib/constructs/google/gtag.js",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^14",
    "jest": "^28.1.2",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^12",
    "prettier": "^2.7.1",
    "standard-version": "^9",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "1.0.1",
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/?(*.)+(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.30.0-alpha.0",
    "aws-cdk-lib": "^2.30.0",
    "constructs": "^10.1.43",
    "prettier": "^2.7.1"
  },
  "description": "A CDK construct for creating proxies over analytic frameworks like Google Analytics (i.e. Google Tag Manager) and Meta Events.",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pinegraph/Analytics-Proxy-CDK.git"
  },
  "keywords": [
    "gtm",
    "google",
    "tag",
    "manager",
    "ad",
    "blocker",
    "ad",
    "meta",
    "meta",
    "events",
    "facebook",
    "facebook",
    "events"
  ],
  "author": "YoshikiMiki",
  "bugs": {
    "url": "https://github.com/Pinegraph/Analytics-Proxy-CDK/issues"
  },
  "homepage": "https://github.com/Pinegraph/Analytics-Proxy-CDK#readme"
}
